{
	"manifest_version": 3,
	"name": "Uwu web",
	"version": "0.1.0",
	"description":"The web is not uwu enough. This extension corrects that.",
	"host_permissions": ["http://*/*", "https://*/*"],
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["http://*/*", "https://*/*"],
			"all_frames": true,
			"run_at": "document_end"
		}
	]
}
